namespace ConsoleApp102
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int number = 42;
            Int32 boxed = number; //Boxing
            int unboxed = boxed; //UnBoxing

            object obj = number; //UpCasting
            int downed = (int)obj; //강제형변환, DownCastiong

        }
    }
